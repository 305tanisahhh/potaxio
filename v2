local Lighting = game:GetService("Lighting")
local Players = game:GetService("Players")
local RunService = game:GetService("RunService")
local VIM = game:GetService("VirtualInputManager")
local UserInputService = game:GetService("UserInputService")
local StarterGui = game:GetService("StarterGui")

--Checks
if Potaxio then
	StarterGui:SetCore("SendNotification",{
		Title = "Potaxio Assistant",
		Text = "You already ran the script",
		Icon = "http://www.roblox.com/asset/?id=7545971320"
	})
	return
end

if game.PlaceId ~= 12062942034 then
	StarterGui:SetCore("SendNotification",{
		Title = "Potaxio Assistant",
		Text = "The script should only run in Heroes: Online World",
		Icon = "http://www.roblox.com/asset/?id=7545971320"
	})
	return
end

local mantisVPF = Players.LocalPlayer.PlayerGui.Menu.SelectionUI.Selection.ScrollBackground.ScrollingFrame.Mantis.ViewportFrame

local function checkCurrentCamera()
	if not mantisVPF.CurrentCamera then
		StarterGui:SetCore("SendNotification", {
			Title = "Potaxio Assistant",
			Text = "Load your characters to prevent data corruption, then run it",
			Icon = "http://www.roblox.com/asset/?id=7545971320"
		})
		return
	else
		pcall(function() getgenv().Potaxio = true end)
	end
end

checkCurrentCamera()
if not Potaxio then
	return
end

mantisVPF:GetPropertyChangedSignal("CurrentCamera"):Connect(function()
	checkCurrentCamera()
end)

if Players.LocalPlayer.PlayerGui:FindFirstChild("FarmingAid") then
	Players.LocalPlayer.PlayerGui.FarmingAid:Destroy()
	StarterGui:SetCore("SendNotification", {
		Title = "Potaxio Assistant",
		Text = "The old GUI has been destroyed... How?",
		Icon = "http://www.roblox.com/asset/?id=7545971320"
	})
end

