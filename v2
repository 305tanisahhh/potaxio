for i,v in pairs(Potaxio:GetDescendants()) do
	if v:IsA("TextButton") or v:IsA("TextBox") then
		local UIStroke = Instance.new("UIStroke")
		UIStroke.Color = Color3.fromRGB(172, 171, 172)
		UIStroke.Thickness = 1
		UIStroke.ApplyStrokeMode = Enum.ApplyStrokeMode.Border
		UIStroke.Parent = v
	elseif v:IsA("Frame") and v.Name == "MainFrame" then
		local UIStroke = Instance.new("UIStroke")
		UIStroke.Color = Color3.fromRGB(172, 171, 172)
		UIStroke.Thickness = 2
		UIStroke.ApplyStrokeMode = Enum.ApplyStrokeMode.Border
		UIStroke.Parent = v
	end

end

--Variables for Functions
local player = Players.LocalPlayer
local playerGui = player:FindFirstChild("PlayerGui")
local playerName

local camera = workspace.Camera
local cameraCFrame
local cameraConnection

local character
local humanoid

local states = {
	teleportState = false,
	clickState = false,
	deathState = false
}

local coreGui = true

local cooldown = false

local charsGui = false

local clicksPerSecond = 0.1

local xShift = 0
local yShift = 0
local zShift = 0

local detectionRadius = 3072
local leaveRadius = 0

local skinsFrame = Players.LocalPlayer.PlayerGui.Menu.SelectionUI.Skins
skinsFrame.AutomaticSize = Enum.AutomaticSize.X
local skin = Players.LocalPlayer.PlayerGui.Menu.SelectionUI.Skins.PromRaven
local deploy = Players.LocalPlayer.PlayerGui.Menu.SelectionUI.Deploy
local VIM = game:GetService("VirtualInputManager")

Lighting.ColorDeath:Destroy()
Lighting.BlurDeath:Destroy()

--Main
local function updateStatus(stateName, value)
	local color = value and Color3.new(0, 1, 0) or Color3.new(1, 0, 0)
	if stateName == "teleportState" then
		TeleportStatus.BackgroundColor3 = color
	elseif stateName == "clickState" then
		DeployStatus.BackgroundColor3 = color
	elseif stateName == "deathState" then
		DeathStatus.BackgroundColor3 = color
	end
end

setmetatable(states, {
	__newindex = function(table, key, value)
		rawset(table, key, value)
		updateStatus(key, value)
	end
})

local function teleportToPlayer()
	TeleportStatus.BackgroundColor3 = Color3.new(0,1,0)
	while states.teleportState do
		local targetPlayer = game.Players:FindFirstChild(playerName)
		if targetPlayer and targetPlayer.Character then
			local humanoidRootPart = targetPlayer.Character:FindFirstChild("HumanoidRootPart")
			if humanoidRootPart and player.Character then
				local newX = humanoidRootPart.Position.X + xShift
				local newY = humanoidRootPart.Position.Y + yShift
				local newZ = humanoidRootPart.Position.Z + zShift
				local newPosition = Vector3.new(newX, newY, newZ)
				game.Players.LocalPlayer.Character:MoveTo(newPosition)
			end
		end
		task.wait()
	end
	TeleportStatus.BackgroundColor3 = Color3.new(1,0,0)
end

local function autoDeploy()
	DeployStatus.BackgroundColor3 = Color3.new(0,1,0)
	while states.clickState do
		if not player or not player.Character or not player.Character:FindFirstChild("Humanoid") then
			skinsFrame.Visible = true
			deploy.Visible = true
			skin.Visible = true
			VIM:SendMouseButtonEvent(skin.AbsolutePosition.X + skin.AbsoluteSize.X / 2, skin.AbsolutePosition.Y + skin.AbsoluteSize.Y / 2, 0, true, nil, 0)
			VIM:SendMouseButtonEvent(skin.AbsolutePosition.X + skin.AbsoluteSize.X / 2, skin.AbsolutePosition.Y + skin.AbsoluteSize.Y / 2, 0, false, nil, 0)
			task.wait(clicksPerSecond)
			VIM:SendMouseButtonEvent(deploy.AbsolutePosition.X + deploy.AbsoluteSize.X / 2, deploy.AbsolutePosition.Y + deploy.AbsoluteSize.Y / 2, 0, true, nil, 0)
			VIM:SendMouseButtonEvent(deploy.AbsolutePosition.X + deploy.AbsoluteSize.X / 2, deploy.AbsolutePosition.Y + deploy.AbsoluteSize.Y / 2, 0, false, nil, 0)
		end	
		task.wait()
	end
	DeployStatus.BackgroundColor3 = Color3.new(1,0,0)
end

TeleportButton.MouseButton1Click:Connect(function()
	if not playerName then
		StarterGui:SetCore("SendNotification",{
			Title = "Potaxio Assistant",
			Text = "Enter a username to start farming",
			Icon = "http://www.roblox.com/asset/?id=7545971320"
		})
		return
	end

	states.teleportState = not states.teleportState

	if states.teleportState then
		coroutine.wrap(teleportToPlayer)()
	end
end)

DeployButton.MouseButton1Click:Connect(function()
	if not playerName then
		StarterGui:SetCore("SendNotification",{
			Title = "Potaxio Assistant",
			Text = "Enter a username to start farming",
			Icon = "http://www.roblox.com/asset/?id=7545971320"
		})
		return
	end

	states.clickState = not states.clickState

	if states.clickState then
		coroutine.wrap(autoDeploy)()
	end
end)

local function killOnHit()
	local humanoid = Players.LocalPlayer.Character:WaitForChild("Humanoid")
	local isPlayerDead = false
	
	if states.deathState == true then
		DeathStatus.BackgroundColor3 = Color3.new(0,1,0)
	elseif states.deathState == false then
		DeathStatus.BackgroundColor3 = Color3.new(1,0,0) 
	else
		DeathStatus.BackgroundColor3 = Color3.new(1,1,0)
	end

	humanoid.HealthChanged:Connect(function()
		if not isPlayerDead and states.deathState then
			humanoid.Health = 0
		end
	end)

	humanoid.Died:Connect(function()
		isPlayerDead = true
		T.Visible = false
	end)
end

DeathButton.MouseButton1Click:Connect(function()
	if not playerName then
		StarterGui:SetCore("SendNotification", {
			Title = "Potaxio Assistant",
			Text = "Enter a username to start farming",
			Icon = "http://www.roblox.com/asset/?id=7545971320"
		})
		return
	end

	states.deathState = not states.deathState

	if player.Character then
		killOnHit(player.Character)
	end
end)

AllButton.MouseButton1Click:Connect(function()
	if not playerName then
		StarterGui:SetCore("SendNotification",{
			Title = "Potaxio Assistant",
			Text = "Enter a username to start farming",
			Icon = "http://www.roblox.com/asset/?id=7545971320"
		})
		return
	end
	
	states.teleportState = true
	states.clickState = true
	states.deathState = true
	
	coroutine.wrap(teleportToPlayer)()
	coroutine.wrap(autoDeploy)()
	killOnHit()
end)

DisableButton.MouseButton1Click:Connect(function()
	states.teleportState = false
	states.clickState = false
	states.deathState = false
end)

ChatSpyButton.MouseButton1Click:Connect(function()
	loadstring(game:HttpGet(('https://rawscripts.net/raw/Universal-Script-Chat-spy-3000'),true))()
end)

InfiniteYieldButton.MouseButton1Click:Connect(function()
	loadstring(game:HttpGet(('https://raw.githubusercontent.com/EdgeIY/infiniteyield/master/source'),true))()
end)

--Other
BlackButton.MouseButton1Click:Connect(function()
	local black = Lighting:FindFirstChild("BlackScreen")
	if black then
		black:Destroy()
	else
		local black = Instance.new("ColorCorrectionEffect")
		black.Brightness = -1
		black.Name = "BlackScreen"
		black.Parent = Lighting
	end
end)

LowGFXButton.MouseButton1Click:Connect(function()
	local Terrain = workspace:FindFirstChildOfClass('Terrain')
	Terrain.WaterWaveSize = 0
	Terrain.WaterWaveSpeed = 0
	Terrain.WaterReflectance = 0
	Terrain.WaterTransparency = 0
	Lighting.GlobalShadows = false
	Lighting.FogEnd = 9e9
	Lighting.FogEnd = 100000

	for i,v in pairs(Lighting:GetDescendants()) do
		if v:IsA("Atmosphere") then
			v:Destroy()
		end
	end

	settings().Rendering.QualityLevel = 1

	for i,v in pairs(game:GetDescendants()) do
		if v:IsA("Part") or v:IsA("UnionOperation") or v:IsA("MeshPart") or v:IsA("CornerWedgePart") or v:IsA("TrussPart") then
			v.Material = "Plastic"
			v.Reflectance = 0
		elseif v:IsA("Decal") then
			v.Transparency = 1
		elseif v:IsA("ParticleEmitter") or v:IsA("Trail") then
			v.Lifetime = NumberRange.new(0)
		elseif v:IsA("Explosion") then
			v.BlastPressure = 1
			v.BlastRadius = 1
		end
	end

	for i,v in pairs(game.Lighting:GetDescendants()) do
		if v:IsA("BlurEffect") or v:IsA("SunRaysEffect") or v:IsA("ColorCorrectionEffect") or v:IsA("BloomEffect") or v:IsA("DepthOfFieldEffect") then
			v.Enabled = false
		end
	end

	workspace.DescendantAdded:Connect(function(child)
		task.spawn(function()
			if child:IsA('ForceField') then
				RunService.Heartbeat:Wait()
				child:Destroy()
			elseif child:IsA('Sparkles') then
				RunService.Heartbeat:Wait()
				child:Destroy()
			elseif child:IsA('Smoke') or child:IsA('Fire') then
				RunService.Heartbeat:Wait()
				child:Destroy()
			end
		end)
	end)
end)

local function lockCamera()
	cameraConnection = function()
		while cameraState == true do
			camera.CameraType = Enum.CameraType.Custom
		end
	end
end

CameraButton.MouseButton1Click:Connect(function()
	if cameraState == false then
		cameraState = true
		lockCamera()
	else
		cameraState = false
		if cameraConnection then
			cameraConnection:Disconnect()
		end
	end
end)

CharsGuiButton.MouseButton1Click:Connect(function()
	local selection = Players.LocalPlayer.PlayerGui.Menu.SelectionUI.Selection
	local statistics = Players.LocalPlayer.PlayerGui.Menu.SelectionUI.Statistics
	
	if charsGui == false then
		selection.Visible = false
		statistics.Visible = false
	elseif charsGui == true then
		selection.Visible = true
		statistics.Visible = true
	end
end)

CoreGuiButton.MouseButton1Click:Connect(function()
	if coreGui == false then
		coreGui = true
		StarterGui:SetCoreGuiEnabled(Enum.CoreGuiType.All, true)
	elseif coreGui == true then
		coreGui = false
		StarterGui:SetCoreGuiEnabled(Enum.CoreGuiType.All, false)
	end
end)

ClicksBox.FocusLost:Connect(function()
	local inputClicksText = ClicksBox.Text
	clicksPerSecond = (1/inputClicksText)
	
end)

xBox.FocusLost:Connect(function()
	xShift = xBox.Text
end)

yBox.FocusLost:Connect(function()
	yShift = yBox.Text
end)

zBox.FocusLost:Connect(function()
	zShift = zBox.Text
end)

DetectButton.MouseButton1Click:Connect(function()
	local hitbox = Instance.new("Part")
	hitbox.Size = Vector3.new(detectionRadius, detectionRadius, detectionRadius)
	hitbox.Position = player.Character.HumanoidRootPart.Position
	hitbox.Anchored = true
	hitbox.CanCollide = false
	hitbox.Transparency = 1
	hitbox.Parent = workspace
	local parts = workspace:GetPartBoundsInBox(hitbox.CFrame, hitbox.Size)
	for i,v in pairs(parts) do
		if v.Parent:FindFirstChild("Humanoid") then
			local player = game.Players:GetPlayerFromCharacter(v.Parent)
		end
	end
end)

