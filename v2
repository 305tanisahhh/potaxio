	zShift = zBox.Text
end)

DetectButton.MouseButton1Click:Connect(function()
	local hitbox = Instance.new("Part")
	hitbox.Size = Vector3.new(detectionRadius, detectionRadius, detectionRadius)
	hitbox.Position = player.Character.HumanoidRootPart.Position
	hitbox.Anchored = true
	hitbox.CanCollide = false
	hitbox.Transparency = 1
	hitbox.Parent = workspace
	local parts = workspace:GetPartBoundsInBox(hitbox.CFrame, hitbox.Size)
	for i,v in pairs(parts) do
		if v.Parent:FindFirstChild("Humanoid") then
			local player = game.Players:GetPlayerFromCharacter(v.Parent)
		end
	end
end)

