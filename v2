UserInputService.InputBegan:Connect(function(input, gameProcessed)
	if gameProcessed then
		return
	end

	if input.KeyCode == Enum.KeyCode.T then
		MainFrame.Visible = not MainFrame.Visible
	end
end)

Players.PlayerAdded:Connect(function(player)
	player.IsInGroup("15346982")
	Players.LocalPlayer:Kick("A user of conflice has joined the server (tester/admin)")
end)

local function handlePlayerDeath(character)
	local humanoid = character:WaitForChild("Humanoid")
	local isPlayerDead = false
	local debounce = false

	local function onHealthChanged(health)
		if not isPlayerDead and killState and not debounce then
			debounce = true
			humanoid.Health = 0
			wait(1)
			debounce = false
		end
	end

	local function onDeath()
		isPlayerDead = true
	end

	humanoid.HealthChanged:Connect(onHealthChanged)
	humanoid.Died:Connect(onDeath)
end

local function waitForRespawn()
	local character = player.Character or player.CharacterAdded:Wait()
	handlePlayerDeath(character)
	while teleportState == true do
		teleportToPlayer()
		wait()
	end
end


player.CharacterAdded:Connect(function(character)
handlePlayerDeath(character)
end)

player.CharacterAdded:Connect(waitForRespawn)
