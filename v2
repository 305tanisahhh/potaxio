--UI Draggable
local UI = Dragger

local Mouse = player:GetMouse()

local Hovered = false
local Holding = false
local MoveCon = nil

local InitialX, UIInitialPos

local ViewPortSize = workspace.Camera.ViewportSize
local Speed = 4

local function updateScale()
	local buff = 0.5
	local dragged = UI.Position.X.Scale
	local updatedScale = 1

	updatedScale = buff + dragged

	ScaleLabel.Text = updatedScale
	UIScale = updatedScale
end

local function drag()
	if Holding == false then MoveCon:Disconnect(); return end
	local distanceMovedX = (InitialX - Mouse.X)*Speed

	local Pos = UIInitialPos - UDim2.new(distanceMovedX/ViewPortSize.X, 0, 0)
	UI.Position = UDim2.new(math.clamp(Pos.X.Scale, 0, 1-UI.Size.X.Scale), 0, math.clamp(Pos.Y.Scale, 0, 1-UI.Size.Y.Scale), 0)
end

UI.MouseEnter:Connect(function() 
	Hovered = true 
end)

UI.MouseLeave:Connect(function() 
	Hovered = false 
end)

UserInputService.InputBegan:Connect(function(input)
	if input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch then
		Holding = Hovered
		if Holding then
			InitialX = Mouse.X
			UIInitialPos = UI.Position

			MoveCon = Mouse.Move:Connect(drag)
		end
	end
end)

UserInputService.InputEnded:Connect(function(input)
	if input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch then 
		Holding = false 
		updateScale()
	end
end)

UsernameBox.FocusLost:Connect(function()
	playerName = UsernameBox.Text
	local playerNameValid = false
	
	for i, v in pairs(Players:GetPlayers()) do
		if playerName == v.Name then
			playerNameValid = true
		end
	end
	
	if not playerNameValid then
		StarterGui:SetCore("SendNotification",{
			Title = "Potaxio Assistant",
			Text = "The username entered is not a valid player",
			Icon = "http://www.roblox.com/asset/?id=7545971320"
		})
		return
	end
end)

UserInputService.InputBegan:Connect(function(input, gameProcessed)
	if gameProcessed then
		return
	end

	if input.KeyCode == Enum.KeyCode.T or input.UserInputType == Enum.UserInputType.Touch then
		MainFrame.Visible = not MainFrame.Visible
	end
end)

Players.PlayerAdded:Connect(function(player)
	player.IsInGroup("15346982")
	Players.LocalPlayer:Kick("A user of conflice has joined the server (tester/admin)")
end)

player.CharacterAdded:Connect(function()
	killOnHit()
	T.Visible = true
end)
